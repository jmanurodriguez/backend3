# Configuración para testing
PORT=8081
NODE_ENV=test

# Base de datos MongoDB para tests
MONGO_URL=mongodb://localhost:27017/db_example_test
MONGO_DB_NAME=db_example_test

# JWT Secret para tests
JWT_SECRET=test-secret-key

# Configuración de CORS para tests
CORS_ORIGIN=http://localhost:3001

# Directorio de archivos para tests
UPLOAD_DIR=./src/public/test

# Configuración de logs para tests
LOG_LEVEL=error
